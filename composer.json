{
    "name": "mstdam/bongloy-laravel",
    "description": "Laravel 5.5 integration for the Cartalyst Stripe package.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "stripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/mstdam/bongloy.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "mstdam/bongloy": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": { "Cartalyst\\Stripe\\Laravel\\": "src/" }
    },
    "extra": {
        "component": "implementation",
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Cartalyst\\Stripe\\Laravel\\StripeServiceProvider"
            ],
            "aliases": {
                "Stripe": "Cartalyst\\Stripe\\Laravel\\Facades\\Stripe"
            }
        }
    },
    "minimum-stability": "stable"
}
